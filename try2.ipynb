    "    def inside(a,b):\n",
    "        if b==0:\n",
    "            print(\"No se puede dividir entre 0\")\n",
    "            return\n",
    "        func(a,b)\n",
    "    return inside\n",
    "def div(a,b):\n",
    "    return a/b\n",
    "div = check(div)\n",
    "\n",
    "print(div(10,4))"
